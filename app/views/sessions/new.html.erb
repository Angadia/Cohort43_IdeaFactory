      <h1>Sign In</h1>
      <%= form_with url: session_path do |f| %>
        <div class="form-group">
          <%= f.label :email %>
          <%= f.email_field :email, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :password %>
          <%= f.password_field :password, class: "form-control" %>
        </div>
        <div>
          <%= f.submit "Log In", class: "btn btn-outline-primary btn-lg" %>
        </div>
      <% end %>
